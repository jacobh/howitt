  "query": "\n                    SELECT\n                        m.*,\n                        mr.ride_ids,\n                        mr.route_ids,\n                        mr.trip_ids,\n                        mr.poi_ids\n                    FROM media m\n                    INNER JOIN media_relations mr ON mr.id = m.id\n                    INNER JOIN ride_media rm ON rm.media_id = m.id\n                    WHERE rm.ride_id = $1\n                    ORDER BY m.created_at DESC\n                    ",
