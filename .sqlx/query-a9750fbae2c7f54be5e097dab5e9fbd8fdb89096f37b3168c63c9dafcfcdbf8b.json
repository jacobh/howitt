{
  "db_name": "PostgreSQL",
  "query": "\n                        SELECT \n                            t.*,\n                            COALESCE(array_agg(tr.ride_id) FILTER (WHERE tr.ride_id IS NOT NULL), ARRAY[]::uuid[]) as ride_ids\n                        FROM trips t\n                        LEFT JOIN trip_rides tr ON tr.trip_id = t.id\n                        WHERE user_id = $1 AND slug = $2\n                        GROUP BY t.id, t.name, t.created_at, t.user_id\n                    ",
